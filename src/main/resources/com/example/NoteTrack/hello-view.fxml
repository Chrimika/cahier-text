<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="232.0" prefWidth="466.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.NoteTrack.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="inputUsername" alignment="BOTTOM_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="42.0" prefWidth="426.0" />
    <VBox alignment="BASELINE_CENTER">
        <Label>Password:</Label><TextField fx:id="inputPassword" onKeyPressed="#HidePassword" />
    </VBox>

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" style="-fx-background-color: #162d3a;" text="Login" textFill="WHITE">
      <font>
         <Font size="14.0" />
      </font>
    </Button>
     <Label>Username:</Label>


</VBox>
